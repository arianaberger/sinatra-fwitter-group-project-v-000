<center>
<h2>EDIT TWEET</h2>

<p>
<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
</p>


<form action="/tweets/<%=@tweet.id%>" method='POST'>
  <input type="hidden" name="_method" value="PATCH">
  <input type='text' name='content' value='<%=@tweet.content%>'>
  <input type='submit' value='submit'>
</form>

</center>
